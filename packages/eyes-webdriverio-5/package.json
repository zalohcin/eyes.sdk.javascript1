{
  "name": "@applitools/eyes-webdriverio",
  "version": "5.9.12",
  "description": "Applitools Eyes SDK for WebdriverIO",
  "keywords": [
    "eyes-webdriverio",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "webdriverio"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "directories": {
    "example": "./example",
    "lib": "./src",
    "test": "./test"
  },
  "main": "./index.js",
  "dependencies": {
    "@applitools/dom-utils": "4.7.4",
    "@applitools/eyes-sdk-core": "6.0.10",
    "@applitools/visual-grid-client": "13.5.15",
    "@applitools/eyes-common": "3.18.3",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "webdriverio": "^5.18.6"
  },
  "devDependencies": {
    "@applitools/sdk-test-kit": "^1.0.17",
    "chromedriver": "^80.0.0",
    "eslint": "^6.8.0",
    "geckodriver": "^1.19.1",
    "mocha": "^6.2.2",
    "mocha-parallel-tests": "^2.2.2",
    "shared-examples-for": "^0.1.3"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "vv": "node ../../scripts/verify-versions.js",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "mocha --no-timeouts \"test/+(TestClassicApi*.js|TestFluentApi*.js|TestVisualGrid*.js)\"",
    "test:it": "mocha --no-timeouts \"test/it/*.js\"",
    "test:unit": "mocha \"test/unit/**/*.js\"",
    "test:coverage": "coverage-tests run --path test/coverage/index.js",
    "test:coverage:nuke": "coverage-tests nuke",
    "postpublish": "git push; git push --tags"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=8.9.0"
  }
}