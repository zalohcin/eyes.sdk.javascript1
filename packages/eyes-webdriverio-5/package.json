{
  "name": "@applitools/eyes-webdriverio",
  "version": "5.10.1",
  "description": "Applitools Eyes SDK for WebdriverIO",
  "keywords": [
    "eyes-webdriverio",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "webdriverio"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "directories": {
    "example": "./example",
    "lib": "./src",
    "test": "./test"
  },
  "main": "./index.js",
  "dependencies": {
    "@applitools/dom-utils": "4.7.14",
    "@applitools/eyes-sdk-core": "9.2.1",
    "@applitools/visual-grid-client": "13.7.2",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "webdriverio": "^5.18.6"
  },
  "devDependencies": {
    "@applitools/sdk-fake-eyes-server": "^2.0.0",
    "@applitools/sdk-release-kit": "0.1.0",
    "@applitools/sdk-coverage-tests": "1.0.0",
    "chromedriver": "^80.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "geckodriver": "^1.19.1",
    "mocha": "^6.2.2",
    "mocha-parallel-tests": "^2.2.2",
    "shared-examples-for": "^0.1.3",
    "mocha-parallel-tests": "^2.2.2",
    "spec-xunit-file": "0.0.1-3"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "mocha --no-timeouts \"test/+(TestClassicApi*.js|TestFluentApi*.js|TestVisualGrid*.js)\"",
    "test:it": "mocha --no-timeouts \"test/it/*.js\"",
    "test:unit": "mocha \"test/unit/**/*.js\"",
    "coverage:sandbox": "coverage-tests create-tests && yarn test:coverage && coverage-tests process-report",
    "coverage:prod": "coverage-tests create-tests && yarn test:coverage && coverage-tests process-report --send-report prod",
    "test:coverage": "yarn start-chromedriver && env APPLITOOLS_BATCH_ID=uuidgen XUNIT_FILE=coverage-test-report.xml mocha-parallel-tests --no-timeouts --reporter spec-xunit-file --max-parallel 15 test/coverage/generic/*.spec.js",
    "test:coverage:custom": "mocha \"test/coverage/custom/**/*.spec.js\" --no-timeouts",
    "render": "node scripts/render.js",
    "preversion": "bongo preversion && yarn coverage:prod",
    "version": "bongo version",
    "postversion": "bongo postversion",
    "start-chromedriver": "chromedriver --port=4444 --url-base=wd/hub --silent &",
    "stop-chromedriver": "ps ax | grep chromedriver | grep port=4444 | awk '{print $1}' | xargs kill -9"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=8.9.0"
  }
}
