{
  "name": "@applitools/dom-snapshot",
  "version": "3.4.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test:mocha": "mocha --no-timeouts tests/**/*.test.js",
    "eslint:node": "eslint '**/*.js' --ignore-pattern 'src/browser'",
    "eslint:browser": "eslint -c 'src/browser/.eslintrc.json' 'src/browser/**/*.js'",
    "eslint": "npm run eslint:node && npm run eslint:browser",
    "test": "npm run eslint && npm run test:mocha",
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -c rollup.config.js --watch",
    "surge": "surge tests/fixtures applitools-dom-snapshot.surge.sh",
    "changelog": "git changelog -x -p  | grep  -e '^* ds[,:]' > History.md && git add ./History.md && git commit -m 'ds: changelog'",
    "changelog:init": "git config changelog.format \"* %s [[%h]($(echo $npm_package_repository_url|cut -d+ -f2|cut -d. -f1-2)/commit/%H)]\""
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/applitools/mono/"
  },
  "devDependencies": {
    "@applitools/eslint-plugin-compat": "^0.7.0",
    "@applitools/monitoring-commons": "^1.0.19",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "canvas": "^2.6.1",
    "chai": "^4.2.0",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "css-tree": "^1.0.0-alpha.39",
    "cssom": "^0.4.4",
    "eslint": "5.7.0",
    "eslint-plugin-mocha-no-only": "1.0.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "3.0.0",
    "express": "^4.17.1",
    "jsdom": "^13.2.0",
    "mocha": "5.2.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "puppeteer": "^1.20.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-ignore": "^1.0.5",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-polyfill": "^2.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "url-polyfill": "^1.1.7",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "@applitools/functional-commons": "^1.5.4"
  }
}
