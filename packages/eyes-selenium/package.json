{
  "name": "@applitools/eyes-selenium",
  "version": "4.43.1",
  "description": "Applitools Eyes SDK for Selenium WebDriver",
  "keywords": [
    "eyes-selenium",
    "eyes-rendering",
    "eyes-visual-grid",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "selenium",
    "webdriver",
    "webdriverjs"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "directories": {
    "example": "./example",
    "lib": "./lib",
    "test": "./test"
  },
  "main": "./index.js",
  "dependencies": {
    "@applitools/eyes-sdk-core": "12.0.1",
    "@applitools/visual-grid-client": "14.6.0"
  },
  "devDependencies": {
    "@applitools/sdk-coverage-tests": "1.0.12",
    "@applitools/sdk-fake-eyes-server": "^2.0.0",
    "@applitools/sdk-release-kit": "0.2.2",
    "@applitools/sdk-shared": "0.1.0",
    "assert-rejects": "^1.0.0",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^84.0.0",
    "mocha": "^8.0.1",
    "module-alias": "^2.2.2",
    "ncp": "^2.0.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "selenium-webdriver-3": "npm:selenium-webdriver@^3.6.0",
    "selenium-webdriver-4": "npm:selenium-webdriver@^4.0.0-alpha.7",
    "spec-xunit-file": "0.0.1-3",
    "yargs": "^15.3.1"
  },
  "peerDependencies": {
    "selenium-webdriver": ">=3.6.0"
  },
  "scripts": {
    "lint": "eslint . --ext .js --no-eslintrc --config ../../.eslintrc",
    "render": "node ../sdk-shared/src/render.js",
    "test": "yarn test:it",
    "test:it": "mocha -r ./test/util/version-alias.js --no-timeouts --parallel --jobs 15 './test/it/**/*.spec.js'",
    "coverage:generate": "coverage-tests create-tests",
    "coverage:sandbox": "yarn coverage:generate && yarn test:coverage && coverage-tests process-report",
    "coverage:prod": "yarn coverage:generate && yarn test:coverage && coverage-tests process-report --send-report prod",
    "test:coverage": "APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-selenium' APPLITOOLS_BATCH_ID=$(uuidgen) XUNIT_FILE=coverage-test-report.xml mocha --config .mocharc.cvg.js",
    "preversion": "bongo preversion && yarn test && yarn coverage:prod",
    "version": "bongo version",
    "postversion": "bongo postversion"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=8.9.0"
  }
}
