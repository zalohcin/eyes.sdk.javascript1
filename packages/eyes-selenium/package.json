{
  "name": "@applitools/eyes-selenium",
  "version": "4.35.0",
  "description": "Applitools Eyes SDK for Selenium WebDriver",
  "keywords": [
    "eyes-selenium",
    "eyes-rendering",
    "eyes-visual-grid",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "selenium",
    "webdriver",
    "webdriverjs"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "directories": {
    "example": "./example",
    "lib": "./lib",
    "test": "./test"
  },
  "main": "./index.js",
  "dependencies": {
    "@applitools/dom-utils": "4.7.14",
    "@applitools/eyes-sdk-core": "9.2.1",
    "@applitools/visual-grid-client": "13.7.2"
  },
  "devDependencies": {
    "@applitools/sdk-fake-eyes-server": "^2.0.0",
    "@applitools/sdk-release-kit": "0.1.0",
    "@applitools/sdk-coverage-tests": "1.0.0",
    "assert-rejects": "^1.0.0",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^80.0.0",
    "mocha": "^6.2.2",
    "mocha-parallel-tests": "^2.2.2",
    "nock": "^12.0.3",
    "node-fetch": "^2.6.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "yargs": "^15.3.1",
    "spec-xunit-file": "0.0.1-3"
  },
  "peerDependencies": {
    "selenium-webdriver": "^4.0.0-alpha.5"
  },
  "scripts": {
    "lint": "eslint . --ext .js --no-eslintrc --config ../../.eslintrc",
    "test": "yarn test:unit && yarn test:it",
    "render": "node scripts/render.js",
    "test:unit": "mocha --no-timeouts \"test/unit/**/*.spec.js\"",
    "test:it": "mocha --timeout 120000 \"test/it/**/*.spec.js\"",
    "test:e2e": "mocha --no-timeouts \"test/e2e/**/*.spec.js\"",
    "coverage:sandbox": "coverage-tests create-tests && yarn test:coverage && coverage-tests process-report",
    "coverage:prod": "coverage-tests create-tests && yarn test:coverage && coverage-tests process-report --send-report prod",
    "test:coverage": "env APPLITOOLS_BATCH_ID=uuidgen XUNIT_FILE=coverage-test-report.xml mocha-parallel-tests --no-timeouts --reporter spec-xunit-file --max-parallel 15 test/coverage/generic/*.spec.js",
    "test:coverage:custom": "mocha --no-timeouts \"test/coverage/custom/*.spec.js\"",
    "preversion": "bongo preversion && yarn coverage:prod",
    "version": "bongo version",
    "postversion": "bongo postversion"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=8.9.0"
  }
}
