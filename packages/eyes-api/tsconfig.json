{
  "exclude": ["./src/output"],
  "compilerOptions": {
    "outDir": "dist",
    "declarationDir": "types",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,

    "target": "ES2015",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "downlevelIteration": true,
    "stripInternal": true,
    
  },
}